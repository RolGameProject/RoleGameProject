{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "package.json",
        "use": "@vercel/node"   
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",      
        "dest": "/server/api/index.js" 
      },
      {
        "src": "/(.*)",         
        "dest": "/client/public/index.html"     
      }
    ]
  }
